{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/images/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "main.py"
        }
    ]
}
